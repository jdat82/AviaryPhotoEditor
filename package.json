{
  "name": "AviaryPhotoEditor",
  "main": "build/index.html",
  "description": "Demo app for node-webkit and Aviary SDK",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/jan4dev/AviaryPhotoEditor"
  },
  "window": {
